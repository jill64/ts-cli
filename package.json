{
  "name": "@jill64/ts-cli",
  "version": "1.3.2",
  "type": "module",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "npx tsx src/command-test/mock.test.ts",
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "test": "vitest",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w ."
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.1.44",
    "@jill64/prettier-config": "1.0.0",
    "@types/node": "20.16.11",
    "typescript": "5.6.2",
    "vitest": "2.1.2"
  },
  "dependencies": {
    "@jill64/transform": "1.0.3"
  },
  "homepage": "https://github.com/jill64/ts-cli#readme",
  "license": "MIT",
  "bugs": "https://github.com/jill64/ts-cli/issues",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/ts-cli.git",
    "image": "https://opengraph.githubassets.com/9a11ce6aab9fd92723d0829214742b1686f0b5927a92404400c6e502b1ead99b/jill64/ts-cli"
  },
  "description": "ï¼ž Solidly-Typed CLI Router",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "node",
    "type-safe"
  ]
}
